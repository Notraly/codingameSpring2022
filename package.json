{
  "name": "codingame-ts-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:watch": "webpack --watch",
    "build": "webpack",
    "test": "jest",
    "test:watch": "jest --watch",
    "prettify": "prettier --list-different --write \\\\\\\"src/**/*.ts\\\\\\\""
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.5",
    "jest": "^23.4.2",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.0",
    "source-map-support": "^0.5.19",
    "ts-jest": "^23.1.2",
    "ts-loader": "^8.0.14",
    "typescript": "^3.0.1",
    "webpack": "^5.12.2",
    "webpack-cli": "^4.3.1",
    "webpack-source-map-support": "^2.0.1"
  },
  "prettierConfig": {
    "singleQuote": true
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "dependencies": {
    "path": "^0.12.7"
  }
}
